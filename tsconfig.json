{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/api/blogdata/route.js",
    "app/api/stores/route.js",
    "app/api/parts/route.js",
    "app/api/services/route.js",
    "app/api/stories/route.js",
    "app/api/category/route.js",
    "app/api/car-group-signup/route.js",
    "app/api/articles/route.js",
    "app/api/articles/*/route.ts",
    "app/api/store-promotion/route.js",
    "app/api/store-info/route.js",
    "app/api/store-manager/route.js",
    "app/api/videos/route.js",
    "app/api/store-settings/route.js",
    "app/api/deals/route.js",
    "app/api/homepage/route.js",
    "app/api/cars/route.js",
    "app/api/chat/route.js",
    "app/api/authors/route.js",
    "app/api/send-email/route.ts",
    "app/api/generate-comparison/route.ts",
    "app/api/generate-car-information/route.ts",
    "app/api/translate/route.ts",
    "app/api/addListing/route.ts",
    "app/api/prosandcons/route.ts",
    "app/api/generate-video/route.ts",
    "app/api/og-image/route.tsx",
    "app/api/stores/config/route.ts",
    "app/api/work-gallery/route.js",
    "app/api/products/route.js",
    "app/api/appointments/route.js"
],
  "exclude": [
    "node_modules"
  ]
}
